@model ReportsDemoModel
<div style="float: left">
    @Html.Partial("ThumbnailsViewerPartial")
</div>
<div style="height: 499px; overflow: auto; margin-left: 30px; margin-top: 30px; float: left">
    @foreach(var item in ThumbnailsBookmarkFiller.GetItems(Model.Report, "DocumentViewer")) {
        <div style="margin: 10px 0px 20px 10px; text-align: center;">
            <a href="javascript:@item.NavigationScript">
                <img style="border-color:@item.BorderColor;
                            border-width:@item.BorderWidth;
                            width:@item.Width;
                            height:@item.Height"
                     src="@Url.Action("ThumbnailsImageHandler", new { img = item.ImageFileName })"
                     alt="@Html.Encode(item.Text)" />
                <br />
                @Html.Encode(item.Text)
            </a>
        </div>
    }
</div>
