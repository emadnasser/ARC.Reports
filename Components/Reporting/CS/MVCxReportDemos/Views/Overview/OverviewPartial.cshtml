@model ReportsDemoModel
@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "DocumentViewer";
    settings.Report = Model.Report;

    settings.CallbackRouteValues = new { Controller = "Overview", Action = "OverviewPartial" };
    settings.ExportRouteValues = new { Controller = "Overview", Action = "DocumentViewerExportTo" };
}).GetHtml()