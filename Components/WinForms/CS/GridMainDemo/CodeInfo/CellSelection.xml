      <control>
        <name>ceMultiSelect</name>
        <windowcaption>Multi selection</windowcaption>
        <description>Specifies whether multiple cells (rows) can be selected simultaneously.</description>
        <memberlist>ColumnViewOptionsSelection.MultiSelect</memberlist>
        <dtimage/>
      </control>
    </controlentry>
    <controlentry>
      <control>
        <name>icbSelectMode</name>
        <windowcaption>Multi select mode</windowcaption>
        <description>Specifies whether multiple cells or rows can be selected.</description>
