        <dtimage/>
      </control>
    </controlentry>
    <controlentry>
      <control>
        <name>sbRecords</name>
        <windowcaption>Selected records</windowcaption>
        <description>Gets selected rows (cells) and displays their content.</description>
        <memberlist>ColumnView.GetSelectedRows, ColumnView.GetDataRow, GridView.GetSelectedCells, ColumnView.GetRowCellDisplayText</memberlist>
        <dtimage/>
      </control>
    </controlentry>
    <controlentry>
      <control>
        <name>icbTranslucentColors</name>
        <windowcaption>Translucent colors</windowcaption>
        <description>Enables or disables translucent colors for selected records.</description>
