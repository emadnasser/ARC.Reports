<?xml version="1.0" standalone="yes" ?>
<xs:schema id="dsIssueList" targetNamespace="http://www.tempuri.org/dsIssueList.xsd" xmlns:mstns="http://www.tempuri.org/dsIssueList.xsd" xmlns="http://www.tempuri.org/dsIssueList.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="dsIssueList" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Departments">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="Name" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Items">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CreatedDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="CreatorID" type="xs:int" minOccurs="0" />
							<xs:element name="Description" type="xs:string" minOccurs="0" />
							<xs:element name="FixedDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="ID" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="ModifiedDate" type="xs:dateTime" minOccurs="0" />
							<xs:element name="Name" type="xs:string" minOccurs="0" />
							<xs:element name="OwnerID" type="xs:int" minOccurs="0" />
							<xs:element name="Priority" type="xs:int" minOccurs="0" />
							<xs:element name="ProjectID" type="xs:int" minOccurs="0" />
							<xs:element name="Resolution" type="xs:string" minOccurs="0" />
							<xs:element name="Status" type="xs:int" minOccurs="0" />
							<xs:element name="Type" type="xs:boolean" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Projects">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ID" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="ManagerID" type="xs:int" minOccurs="0" />
							<xs:element name="Name" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProjectTeam">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Function" type="xs:string" minOccurs="0" />
							<xs:element name="ID" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="ProjectID" type="xs:int" minOccurs="0" />
							<xs:element name="UserID" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Users">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Address" type="xs:string" minOccurs="0" />
							<xs:element name="City" type="xs:string" minOccurs="0" />
							<xs:element name="Country" type="xs:string" minOccurs="0" />
							<xs:element name="DepartmentID" type="xs:int" minOccurs="0" />
							<xs:element name="Email" type="xs:string" minOccurs="0" />
							<xs:element name="Fax" type="xs:string" minOccurs="0" />
							<xs:element name="FirstName" type="xs:string" minOccurs="0" />
							<xs:element name="HomePage" type="xs:string" minOccurs="0" />
							<xs:element name="ID" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="LastName" type="xs:string" minOccurs="0" />
							<xs:element name="MiddleName" type="xs:string" minOccurs="0" />
							<xs:element name="Phone" type="xs:string" minOccurs="0" />
							<xs:element name="PostalCode" type="xs:string" minOccurs="0" />
							<xs:element name="FullName" type="xs:string" minOccurs="0" msdata:Expression="[FirstName] + iif([MiddleName] is null, ' ', ' ' + [MiddleName] + ' ') + [LastName] " />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Departments" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:unique name="Items_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Items" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:unique name="Projects_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Projects" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:unique name="ProjectTeam_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:ProjectTeam" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:unique name="Users_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Users" />
			<xs:field xpath="mstns:ID" />
		</xs:unique>
		<xs:keyref name="UsersItems" refer="mstns:Users_Constraint1">
			<xs:selector xpath=".//mstns:Items" />
			<xs:field xpath="mstns:CreatorID" />
		</xs:keyref>
		<xs:keyref name="UsersItems1" refer="mstns:Users_Constraint1">
			<xs:selector xpath=".//mstns:Items" />
			<xs:field xpath="mstns:OwnerID" />
		</xs:keyref>
		<xs:keyref name="UsersProjectTeam" refer="mstns:Users_Constraint1">
			<xs:selector xpath=".//mstns:ProjectTeam" />
			<xs:field xpath="mstns:UserID" />
		</xs:keyref>
		<xs:keyref name="UsersProjects" refer="mstns:Users_Constraint1">
			<xs:selector xpath=".//mstns:Projects" />
			<xs:field xpath="mstns:ManagerID" />
		</xs:keyref>
		<xs:keyref name="DepartmentsUsers" refer="mstns:Constraint1">
			<xs:selector xpath=".//mstns:Users" />
			<xs:field xpath="mstns:DepartmentID" />
		</xs:keyref>
		<xs:keyref name="ProjectsProjectTeam" refer="mstns:Projects_Constraint1">
			<xs:selector xpath=".//mstns:ProjectTeam" />
			<xs:field xpath="mstns:ProjectID" />
		</xs:keyref>
		<xs:keyref name="ProjectsItems" refer="mstns:Projects_Constraint1">
			<xs:selector xpath=".//mstns:Items" />
			<xs:field xpath="mstns:ProjectID" />
		</xs:keyref>
	</xs:element>
</xs:schema>