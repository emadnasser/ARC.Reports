	Public Class ProductInfo
		Private productCategories() As String = { "Windows Controls", "Web Controls", "Reporting / Printing Suites"}
		Private productTypes() As String = { "Grids / Data Entry", "Charting / Data Analysis", "Navigation / Layout", "Scheduling / Planning ", "Reporting Solutions", "Printing Libraries", "Text Editors / Spell Checkers"}
		Private name_Renamed, notes_Renamed As String
		Private category_Renamed, type_Renamed As Integer
		Private sourcePricing, pricing As Double

		Public Sub New(ByVal name As String, ByVal notes As String, ByVal category As Integer, ByVal type As Integer, ByVal sourcePricing As Double, ByVal pricing As Double)
			Me.name_Renamed = name
			Me.notes_Renamed = notes
			Me.category_Renamed = category
			Me.type_Renamed = type
			Me.sourcePricing = sourcePricing
			Me.pricing = pricing
		End Sub

		Public Property Name() As String
			Get
				Return name_Renamed
			End Get
			Set(ByVal value As String)
				name_Renamed = value
			End Set
		End Property
		Public Property Notes() As String
			Get
				Return notes_Renamed
			End Get
			Set(ByVal value As String)
				notes_Renamed = value
			End Set
		End Property
		Public Property PricingWithSourceCode() As Double
			Get
				Return sourcePricing
			End Get
			Set(ByVal value As Double)
				sourcePricing = value
			End Set
		End Property
		Public Property PricingWithoutSourceCode() As Double
			Get
				Return pricing
			End Get
			Set(ByVal value As Double)
				pricing = value
			End Set
		End Property
		Public ReadOnly Property Category() As String
			Get
				Return productCategories(category_Renamed)
			End Get
		End Property
		Private Function IsValue(ByVal val As Integer, ByVal pos As Integer) As Boolean
			Return (val And 1 << pos) <> 0
		End Function
		Public ReadOnly Property Type() As String
			Get
				Dim ret As String = String.Empty
				For i As Integer = 0 To productTypes.Length - 1
					If IsValue(type_Renamed, i) Then
						Dim ds As String = String.Empty
						If ret <> String.Empty Then
							ds = ", "
						End If
						ret &= ds & productTypes(i)
					End If
				Next i
				Return ret
			End Get
		End Property
	End Class
