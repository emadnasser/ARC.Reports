		Private Shared xtraChartsNotes As String = "{\rtf1\ansi\ansicpg1251\deff0\deflang1049\deflangfe1049\deftab708{\fonttbl{\f0\fswiss\fprq2\fcharset0 Tahoma;}{\f1\fswiss\fprq2\fcharset0 Arial;}{\f2\fswiss\fprq2\fcharset204 Tahoma;}}" & ControlChars.CrLf & "{\colortbl ;\red112\green112\blue112;\red48\green48\blue48;}" & ControlChars.CrLf & "{\*\generator Msftedit 5.41.15.1507;}\viewkind4\uc1\pard\sl264\slmult1\lang1033\f0\fs26 XtraCharts\'99 Suite\par" & ControlChars.CrLf & "\f0\fs16 A universal charting and data presentation system for\b  .NET\b0 , the \b XtraCharts Suite \b0 allows you to address your business needs regardless of platform. From \b Windows Forms \b0 and \i ASP.NET \i0 to the printed page, the \b XtraCharts Suite \b0 offers you a rich collection of chart types, unrivaled design-time experience, and superior object model.\lang1049\f2\fs24\par" & ControlChars.CrLf & "}" & ControlChars.CrLf & ""
		Private Shared xtraLayoutNotes As String = "{\rtf1\ansi\ansicpg1251\deff0\deflang1049\deflangfe1049\deftab708{\fonttbl{\f0\fswiss\fprq2\fcharset0 Tahoma;}{\f1\fswiss\fprq2\fcharset0 Arial;}}" & ControlChars.CrLf & "{\colortbl ;\red112\green112\blue112;\red48\green48\blue48;}" & ControlChars.CrLf & "{\*\generator Msftedit 5.41.15.1507;}\viewkind4\uc1\pard\sl264\slmult1\lang1033\f0\fs26 XtraLayoutControl\'99 Suite\par" & ControlChars.CrLf & "\f0\fs16 The \b XtraLayoutControl \b0 represents an entirely new way in which to design and manage your \i Windows GUI \i0 - with it, you can construct forms with full runtime customization and offer end-users complete control over the layout and appearance of your applications with ease.\par" & ControlChars.CrLf & "}" & ControlChars.CrLf & ""
