<script type="text/javascript">
    function OnFileManagerEndCallback(s, e) {
        $("#EventLog").load('@(Url.Action("CloudProviderLogPartial", "FileManager", new { type = FileManagerProviderType.Dropbox }, Request.Url.Scheme))');
    }
</script>

@using(Html.BeginForm()) {
    @Html.Action("CloudProviderPartial", new { type = FileManagerProviderType.Dropbox });
    <br />
    @Html.Action("CloudProviderLog", new { type = FileManagerProviderType.Dropbox });
}