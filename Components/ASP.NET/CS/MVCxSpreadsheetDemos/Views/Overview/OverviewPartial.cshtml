@Html.DevExpress().Spreadsheet(
    settings => {
        settings.Name = "spreadsheet";
        settings.CallbackRouteValues = new { Controller = "Overview", Action = "OverviewPartial" };
        settings.Width = 585;
        settings.Height = 680;
        settings.ActiveTabIndex = 0;
        settings.ShowConfirmOnLosingChanges = false;
        settings.PreRender = (s, e) => {
            SpreadsheetDemoUtils.CreateOverviewRibbon((MVCxSpreadsheet)s);
        };
    }
).Open(Path.Combine(DirectoryManagmentUtils.CurrentDataDirectory, "MonthlyBudget.xlsx")).GetHtml()