@Html.DevExpress().Spreadsheet(
    settings => {
        settings.Name = "spreadsheet";
        settings.CallbackRouteValues = new { Controller = "Features", Action = "WorksheetProtectionSpreadsheetPartial" };
        settings.Width = Unit.Percentage(100);
        settings.Height = Unit.Pixel(700);
        settings.SettingsDocumentSelector.UploadSettings.Enabled = !Utils.IsSiteMode;
        settings.ActiveTabIndex = 0;
        settings.ShowConfirmOnLosingChanges = false;
        
        settings.PreRender = (s, e) => {
            MVCxSpreadsheet spreadsheet = (MVCxSpreadsheet)s;
            SpreadsheetDemoUtils.HideFileTab(spreadsheet);

        };
    }
).Open(Path.Combine(DirectoryManagmentUtils.CurrentDataDirectory, "Protection.xlsx")).GetHtml()