@section AdditionalResources {
    <style type="text/css">
        .optionsList {
            border-style: none !important;
        }
    </style>
}
<script type="text/javascript">
        function ApplyProtectionSettings() {
            CallbackPanel.PerformCallback();
        }
        function CallbackPanel_OnBeginCallback(s, e) {
            e.customArgs["SelectedValues"] = ProtectionOptionsList.GetSelectedValues().join(',');
        }
</script>

@using(Html.BeginForm()) {
    @Html.Partial("WorksheetProtectionCheckBoxListPartial")
    @Html.Partial("WorksheetProtectionCallbackPanelPartial")
}