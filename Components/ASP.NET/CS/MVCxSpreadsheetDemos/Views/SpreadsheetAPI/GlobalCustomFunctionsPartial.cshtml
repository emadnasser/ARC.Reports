@Html.DevExpress().Spreadsheet(
    settings => {
        settings.Name = "spreadsheet";
        settings.CallbackRouteValues = new { Controller = "SpreadsheetAPI", Action = "GlobalCustomFunctionsPartial" };
        settings.Width = Unit.Percentage(100);
        settings.ActiveTabIndex = 0;
        settings.ShowConfirmOnLosingChanges = false;
		settings.ClientSideEvents.EndSynchronization = "function (s, e) { StartSpreadsheetCellEditing(s); }";
        settings.PreRender = (s, e) => {
            SpreadsheetDemoUtils.HideFileTab((MVCxSpreadsheet)s);
        };
}).Open(Path.Combine(DirectoryManagmentUtils.CurrentDataDirectory, "DiscountCustomFunction.xlsx")).GetHtml()