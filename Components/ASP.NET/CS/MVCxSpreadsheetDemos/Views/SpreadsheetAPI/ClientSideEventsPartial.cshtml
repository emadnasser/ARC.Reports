@Html.DevExpress().Spreadsheet(
    settings => {
        settings.Name = "spreadsheet";
        settings.CallbackRouteValues = new { Controller = "SpreadsheetAPI", Action = "ClientSideEventsPartial" };
        settings.Width = Unit.Percentage(100);
        settings.ShowFormulaBar = false;
        settings.ActiveTabIndex = 0;
        settings.ShowConfirmOnLosingChanges = false;

        settings.RibbonTabs.Add(ClientSideEventsDemoHelper.GetCustomRibbonTab());

        settings.ClientSideEvents.Init = "function(s, e) { DXEventMonitor.Trace(s, e, 'Init'); SetDefaultTrackedEvents();  }";
        settings.ClientSideEvents.SelectionChanged = "function(s, e) { DXEventMonitor.Trace(s, e, 'SelectionChanged') }";
        settings.ClientSideEvents.HyperlinkClick = "function(s, e) { DXEventMonitor.Trace(s, e, 'HyperlinkClick'); e.handled = true; }";
        settings.ClientSideEvents.CustomCommandExecuted = "function(s, e) { DXEventMonitor.Trace(s, e, 'CustomCommandExecuted') }";
        settings.ClientSideEvents.BeginSynchronization = "function(s, e) { DXEventMonitor.Trace(s, e, 'BeginSynchronization') }";
        settings.ClientSideEvents.EndSynchronization = "function(s, e) { DXEventMonitor.Trace(s, e, 'EndSynchronization') }";
    }
).Open(Path.Combine(DirectoryManagmentUtils.CurrentDataDirectory, "ClientSideEvents.xlsx")).GetHtml()