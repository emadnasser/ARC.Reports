@Html.DevExpress().GetStyleSheets(
    new StyleSheet { ExtensionSuite = ExtensionSuite.Editors, AccessibilityCompliant = true }, 
    new StyleSheet { ExtensionSuite = ExtensionSuite.NavigationAndLayout }, 
    // Default theme for some demos
    new StyleSheet { ExtensionSuite = ExtensionSuite.NavigationAndLayout, Theme = "Default" },
    // For theme parameter selector
    new StyleSheet { ExtensionSuite = ExtensionSuite.Editors, Theme = "Metropolis" }
) 
@Html.DevExpress().GetScripts(
    new Script { ExtensionSuite = ExtensionSuite.Editors }, 
    new Script { ExtensionSuite = ExtensionSuite.NavigationAndLayout }
) 