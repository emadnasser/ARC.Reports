@model IEnumerable

@using(Html.BeginForm("ExportWithFormatConditionsResult", "Exporting")) {
    @Html.Partial("ExportWithFormatConditionsOptionsPartial", Model)
    @Html.Partial("ExportWithFormatConditionsPartial", Model)
    <input type="hidden" class="format" style="display: none;" />
}
@section AdditionalResources {
    <script>
        function submitForm(s, e) {
            var input = $('input.format');
            var form = input.closest('form');
            input.attr('name', s.name);
            form.submit();
        }
    </script>
}