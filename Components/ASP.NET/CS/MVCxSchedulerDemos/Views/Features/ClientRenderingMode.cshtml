@model SchedulerDataObject

<script type="text/javascript">
    function OnDemoOptionsChanged(s, e){
        scheduler.Refresh();
    }
    function OnBeginCallback(s, e) {
        e.customArgs['enableClientRender'] = EnableClientRender.GetValue();
        document.getElementById("CommandName").innerHTML = e.command;
        document.getElementById("Time").innerHTML = "working...";
        this.startCallbackTime = performance.now();
    }
    function OnEndCallback(s, e) {
        document.getElementById("Time").innerHTML = Math.round(performance.now() - this.startCallbackTime);
    }
</script>
@using (Html.BeginForm()){
    @Html.Partial("ClientRenderingModeOptionsPartial")
    @Html.Partial("ClientRenderingModePartial", Model)
}