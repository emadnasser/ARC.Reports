@model MVCxTabPage
<table class="tabTemplate">
    <tr>
        <td><img alt="" src="@Url.Content("~/Content/TabControl/SelectedLeft.gif")" /></td>
        <td style="white-space: nowrap; background: url(@Url.Content("~/Content/TabControl/SelectedCenter.gif")) repeat-x;">
            <span class="label">
                @Model.Text
            </span>
        </td>
        <td><img alt="" src="@Url.Content("~/Content/TabControl/SelectedRight.gif")" /></td>
    </tr>
</table>
